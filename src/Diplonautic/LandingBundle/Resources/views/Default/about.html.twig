{% trans_default_domain "general" %}
{% extends 'DiplonauticLandingBundle::layout.html.twig' %}
{% block meta_description %}{%trans%}meta.about{%endtrans%}{% endblock %}
{% block meta_title %}{%trans%}header.company{%endtrans%} - Diplonautic{% endblock %}
{% block title %}{%trans%}header.company{%endtrans%} - Diplonautic{% endblock %}
{% block meta %}
    <meta property="og:title" content="{%trans%}header.company{%endtrans%} - Diplonautic" />
    <meta property="og:url" content="http://www.diplonautic.com{{path(app.request.attributes.get('_route'),
                       app.request.attributes.get('_route_params'))}}" />
    <meta property="og:description" content="{%trans%}meta.about{%endtrans%}" />
    <meta property="og:site_name" content="Diplonautic" />
{% endblock %}
{% block content %}
    {% include "DiplonauticLandingBundle:Default:header.html.twig" %}
    <div class="main">
        
        <section>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xs-10 col-xs-offset-1 col-sm-11 col-sm-offset-0 main-description">
                        <h1 class="h3"><b>{%trans%}about.title{%endtrans%}</b></h1>
                        <div class="divisory hidden-xs"><div class="sub-div1"></div><div class="sub-div2"></div></div>
                        <div class="col-sm-5 pad-left">
                            <p>{%trans%}about.paragraph_one{%endtrans%}</p>
                            <p>{%trans%}about.paragraph_one_two{%endtrans%}</p>
                        </div>
                        <div class="col-sm-5 pad-left">
                            <p>{%trans%}about.paragraph_two{%endtrans%}</p>
                        </div>
                    </div>
                   {% include "DiplonauticLandingBundle:Default:language.html.twig" %}
                    
                </div>
            </div>
        </section>
    </div>
{% endblock %}