{% trans_default_domain "general" %}
{% extends 'DiplonauticLandingBundle::layout.html.twig' %}
{% block meta_description %}{%trans%}meta.index{%endtrans%}{% endblock %}
{% block meta_title %}Diplonautic{% endblock %}

{% block title %}Diplonautic{% endblock %}
{% block content %}
    {% include "DiplonauticLandingBundle:Default:header.html.twig" %}
    <div class="main">
        
        <section>
            <div class="container-fluid">
                <div class="row">
                    <div class="main-index no-padding col-sm-11">
                        <div class="col-xs-10 col-xs-offset-1 col-sm-6 col-sm-offset-0 main-description">
                            <h1 class="h3"><b>{%trans%}index.title{%endtrans%}</b></h1>
                            <div class="divisory hidden-xs"><div class="sub-div1"></div><div class="sub-div2"></div></div>
                            <p>{%trans%}index.we_improve{%endtrans%}</p>
                            <ul>
                                <li>{%trans%}index.list.installation{%endtrans%}</li>
                                <li>{%trans%}index.list.repair{%endtrans%}</li>
                                <li>{%trans%}index.list.maintenance{%endtrans%}</li>
                            </ul>
                            <p>{%trans%}index.electrical{%endtrans%}</p>
                        </div>

                        <div class="col-xs-10 col-xs-offset-1 col-sm-6 col-sm-offset-0 main-marks">
                            {% include "DiplonauticLandingBundle:Default:marks.html.twig" %}
                        </div>
                    </div>
                    {% include "DiplonauticLandingBundle:Default:language.html.twig" %}
                </div>
            </div>
        </section>
    </div>
{% endblock %}
