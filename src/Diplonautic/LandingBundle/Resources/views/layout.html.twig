        <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('bundles/diplonauticlanding/images/favicon/favicon-16x16.png')}}">
        <link rel="manifest" href="{{ asset('bundles/diplonauticlanding/images/favicon/manifest.json')}}">
        <meta name="msapplication-TileColor" content="#ffffff">
        <meta name="msapplication-TileImage" content="{{ asset('bundles/diplonauticlanding/images/favicon/ms-icon-144x144.png')}}">
        <meta name="theme-color" content="#ffffff">
        
        
        <link href='http://fonts.googleapis.com/css?family=Quicksand' rel='stylesheet' type='text/css'>
        <script src="{{ asset("bundles/diplonauticlanding/js/jquery-1.11.3.min.js") }}"></script>
        <script src="{{ asset('bundles/bazingajstranslation/js/translator.min.js') }}"></script>
        <script src="{{ asset('bundles/diplonauticlanding/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('bundles/diplonauticlanding/js/owl.carousel.js') }}"></script>
        <script src="{{ asset('bundles/diplonauticlanding/js/jquery.fitImages.js') }}"></script>
        <script src="{{ asset("bundles/diplonauticlanding/js/main.js") }}"></script>
        
