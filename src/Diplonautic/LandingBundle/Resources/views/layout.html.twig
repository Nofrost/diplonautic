<!doctype html>
<html class="no-js" lang="{{app.request.locale}}">
    <head>
	
        <meta charset="utf-8">

        <title>{% block title %}{% endblock %}</title>

        <meta name="title" content="{% block meta_title %}{% endblock %}">
        <meta name="description" content="{% block meta_description %}{% endblock %}">
        <meta name="keywords" content="{%trans%}keywords{%endtrans%}">
        <meta property="og:image" content=""/>

        <meta http-equiv="Content-Language" content="{{app.request.locale}}"/>
        <meta property="og:type" content="website" />   
        <meta name="Robots" content="all"/>

        <meta property="fb:admins" content="" />


       
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <link rel="stylesheet" href="{{ asset('bundles/diplonauticlanding/css/bootstrap.min.css') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/diplonauticlanding/css/font-awesome.min.css') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/diplonauticlanding/css/owl.carousel.css') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/diplonauticlanding/css/owl.theme.css') }}" />
