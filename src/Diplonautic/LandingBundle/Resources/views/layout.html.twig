        <link rel="stylesheet" href="{{ asset('bundles/diplonauticlanding/css/main.css') }}" />
        
        {#Favicons#}
        <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('bundles/diplonauticlanding/images/favicon/apple-icon-57x57.png')}}">
